<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <TaleDef Abstract="True" Name="RV2_VoreExit_Base" ParentName="RV2_TaleBase">
    <label>finished vore</label>
    <taleClass>Tale_DoublePawnAndDef</taleClass>
    <type>Volatile</type>
    <firstPawnSymbol>PREDATOR</firstPawnSymbol>
    <secondPawnSymbol>PREY</secondPawnSymbol>
    <defSymbol>VOREPATH</defSymbol>
    <baseInterest>2</baseInterest>
    <rulePack>
      <rulesStrings>
        <li>RV2_exit_tale_noun->[PREDATOR_definite] [RV2_exit_predVerb] [PREY_nameDef]</li>
        <li>RV2_exit_tale_noun->[PREDATOR_definite] [RV2_exit_predVerb] [PREY_nameDef] [RV2_adverb]</li>
        <li>tale_noun->[RV2_exit_tale_noun]</li>
        <li>RV2_exit_image->[PREY_definite] [RV2_exit_preyVerb] [PREDATOR_definite]</li>
        <li>RV2_exit_image->[PREY_definite] [RV2_exit_preyVerb] [PREDATOR_definite] [RV2_adverb]</li>
        <li>image->[RV2_exit_image]</li>
        <li>RV2_exit_desc_sentence->[PREDATOR_definite] is preparing for [PREDATOR_possessive] next prey.</li>
        <li>RV2_exit_desc_sentence->[PREDATOR_definite] has found better prey.</li>
        <li>desc_sentence(p=2)->[RV2_exit_desc_sentence]</li>
        <li>RV2_exit_predVerb->dumping</li>
        <li>RV2_exit_preyVerb->being dumped by</li>
      </rulesStrings>
    </rulePack>
  </TaleDef>
