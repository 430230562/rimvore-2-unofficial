<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <RimVore2.VorePathDef>
    <defName>OralHeal</defName>
    <label>(Heal) Oral</label>
    <RMBLabel>Oral vore</RMBLabel>
    <actionDescription>{PREDATOR} will heal {PREY} in their stomach</actionDescription>
    <description>Healing vore will tend to a pawns wounds and slowly heal their damaged body parts. Healing vore is relatively weak, so if you want to treat a pawn properly, use medicine and a good doctor.</description>
    <initTale>RV2_VoreInitiation_OralHeal</initTale>
    <exitTale>RV2_VoreExit_OralHeal</exitTale>
    <voreType>Oral</voreType>
    <voreGoal>Heal</voreGoal>
    <postVoreMemories>
      <predatorPostVoreSuccess>RV2_PredatorOralHealSuccess</predatorPostVoreSuccess>
      <preyPostVoreSuccess>RV2_PreyOralHealSuccess</preyPostVoreSuccess>
    </postVoreMemories>
    <stages>
      <li>Mouth_Enter</li>
      <li>Stomach_Warmup</li>
      <li>Stomach_Heal</li>
      <li>Mouth_Regurgitate</li>
    </stages>
    <defaultRequiredStruggles>9</defaultRequiredStruggles>
  </RimVore2.VorePathDef>

