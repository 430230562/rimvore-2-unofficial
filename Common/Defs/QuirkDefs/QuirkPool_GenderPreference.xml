<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Predator side -->

  <RimVore2.QuirkPoolDef>
    <defName>GenderPreference_Predator</defName>
    <label>Predator gender preference</label>
    <category>Role Preference</category>
    <description>Quirks that dictate what gender preferences a predator might have when choosing prey. Please note that these preferences are not influenced by a pawn's sexuality.</description>
    <generationOrder>3</generationOrder>
    <poolType>PickOne</poolType>
    <quirks>
      <li>GenderPreference_Predator_FemaleOnly</li>
      <li>GenderPreference_Predator_FemalePreferred</li>
      <li>GenderPreference_Predator_Neutral</li>
      <li>GenderPreference_Predator_MalePreferred</li>
      <li>GenderPreference_Predator_MaleOnly</li>
    </quirks>
    <blockingQuirks>
      <li>PredatorDisposition_Refuses</li>
    </blockingQuirks>
    <blockingKeywords>
      <li>PawnIsAnimal</li>
    </blockingKeywords>
  </RimVore2.QuirkPoolDef>

  <RimVore2.QuirkDef>
    <defName>GenderPreference_Predator_FemaleOnly</defName>
    <label>Exclusive Gynophage</label>
    <description>[PAWN_label] exclusively prefers female prey, and will never willingly eat a man.</description>
    <rarity>Rare</rarity>
    <comps>
      <li Class="RimVore2.QuirkComp_VoreTargetSelectorModifier">
        <selectors>
          <gender>Female</gender>
          <role>Predator</role>
        </selectors>
        <modifierValue>4</modifierValue>
      </li>
      <li Class="RimVore2.QuirkComp_VoreTargetSelectorModifier">
        <selectors>
          <gender>Male</gender>
          <role>Predator</role>
        </selectors>
        <modifierValue>-4</modifierValue>
      </li>
      <li Class="RimVore2.QuirkComp_PostVoreMemory">
        <keywords>
          <li>PawnIsPredator</li>
          <li>PreyIsFemale</li>
        </keywords>
        <memory>RV2_GenderPreferenceQuirkSatisfaction_Predator_Female_VeryGood</memory>
      </li>
      <li Class="RimVore2.QuirkComp_PostVoreMemory">
        <keywords>
          <li>PawnIsPredator</li>
          <li>PreyIsMale</li>
        </keywords>
        <memory>RV2_GenderPreferenceQuirkSatisfaction_Predator_Male_Bad</memory>
      </li>
    </comps>
  </RimVore2.QuirkDef>
