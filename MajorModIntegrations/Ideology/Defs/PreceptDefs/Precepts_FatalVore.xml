  <!-- Required -->
  <PreceptDef>
    <defName>RV2_FatalVore_Required</defName>
    <issue>RV2_FatalVore</issue>
    <label>required</label>
    <description>We are born killers, we require regular fatal vore.</description>
    <impact>High</impact>
    <displayOrderInIssue>20</displayOrderInIssue>
    <displayOrderInImpact>1000</displayOrderInImpact>
    <associatedMemes>
      <li>RV2_Preyism</li>
      <li>RV2_Predation</li>
    </associatedMemes>
    <comps>
      <li Class="PreceptComp_SelfTookMemoryThought">
        <eventDef>RV2_FatalVore</eventDef>
        <thought>RV2_FatalVore_Required</thought>
      </li>
      <li Class="PreceptComp_KnowsMemoryThought">
        <eventDef>RV2_FatalVore</eventDef>
        <thought>RV2_FatalVore_Know_Required</thought>
      </li>
      <li Class="PreceptComp_KnowsMemoryThought">
        <eventDef>RV2_FatalVore</eventDef>
        <thought>RV2_FatalVore_Know_Social_Required</thought>
      </li>
      <li Class="PreceptComp_SituationalThought">
        <thought>RV2_NoRecentFatalVore_Required</thought>
        <description>No fatal vore for {VORERELATEDINTERVAL} days</description>
      </li>
      <li Class="RimVore2.PreceptComp_Quirk_RemoveQuirks">
        <quirks>
          <li>GoalPreference_Predator_Digest_Dislikes</li>
          <li>GoalPreference_Predator_Digest_Refuses</li>
          <li>GoalPreference_Prey_Digest_Dislikes</li>
          <li>GoalPreference_Prey_Digest_Refuses</li>
        </quirks>
      </li>
      <li Class="RimVore2.PreceptComp_Quirk_AddQuirks">
        <quirks>
          <li>Enablers_Core_Goal_Digest</li>
        </quirks>
      </li>
    </comps>
  </PreceptDef>

  <ThoughtDef>
    <defName>RV2_FatalVore_Required</defName>
    <durationDays>1</durationDays>
    <stages>
      <li>
        <label>Fatally vored someone</label>
        <description>By voring and killing my prey I honor my beliefs.</description>
        <baseMoodEffect>5</baseMoodEffect>
