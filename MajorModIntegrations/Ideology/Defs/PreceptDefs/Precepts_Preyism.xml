<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <IssueDef>
    <defName>RV2_Preyism</defName>
    <label>Preyism</label>
    <iconPath>UI/Issues/Preyism</iconPath>
  </IssueDef>

