<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <IssueDef>
    <defName>RV2_Feeding</defName>
    <label>Feeding</label>
    <iconPath>UI/Issues/Feeding</iconPath>
  </IssueDef>

  <!-- Neutral -->
