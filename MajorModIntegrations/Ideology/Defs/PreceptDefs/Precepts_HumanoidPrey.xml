<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <IssueDef>
    <defName>RV2_HumanoidPrey</defName>
    <label>Humanoid Prey</label>
    <iconPath>UI/Issues/Humanoid</iconPath>
  </IssueDef>

